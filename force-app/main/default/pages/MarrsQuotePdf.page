                                                                <td style="font-family:Arial,sans-serif;font-size: 10px;display:{!IF(subGrp.productGroup.Hide_Quantity__c == true, 'none', 'table-cell')}">
                                                                    <apex:outputText value="{!prod.productDetails.Quantity__c}" rendered="{! !prod.productDetails.Hide_Quantity__c}" />
                                                                </td>
                                                                -->
                                                                <td style="font-family:Arial,sans-serif;font-size: 10px;border-left: 1px solid #9e9b9b;display:{!IF(Quote.Hide_All_Tags__c == true, 'none', 'table-cell')}">{!prod.productDetails.Tag__c}</td>

                                                                <td colspan="{!subGrp.colspan}" style="font-family:Arial,sans-serif;font-size: 10px;border-left: 1px solid #9e9b9b;">
                                                                    <apex:outputText value="{!prod.productDetails.Descriptions__c}" escape="false" />
                                                                </td>
                                                                <td style="font-family:Arial,sans-serif;font-size: 10px;border-left: 1px solid #9e9b9b;display:{!IF(subGrp.productGroup.Hide_Price_Column__c == true, 'none', 'table-cell')}">
