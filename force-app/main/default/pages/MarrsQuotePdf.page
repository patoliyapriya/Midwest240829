                                                    </tr>
                                                    <apex:variable var="color" value="#F2F2F2" />
                                                    <apex:repeat value="{!subGrp.lstProductSelected}" var="prod">
                                                        <apex:variable var="color" value="{!IF(color == '#F2F2F2', '', '#F2F2F2')}" />
